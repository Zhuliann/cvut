// Spolecny soubor, kde budeme obe delat zmeny.


#include <cmath>
#include <iostream>


using namespace std;

class Polymer {
	protected:
		float theta;
		float b;
		float c;
				
	public:
		Polymer(float c1, float b1){
			theta = acos((c1 - 1) / (1 + c1));
			b = b1;
			c = c1;
		};

		float** runSimulation(int n){
			float **result = new float*[n];
			result[0] = new float[2];
			result[0][0] = 0.0;
			result[0][1] = 0.0;
		

};	
